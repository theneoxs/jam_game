; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="jam_game"
run/main_scene="res://Scenes/Start_screen.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://Res/icon.svg"

[autoload]

Global="*res://global.tscn"
Net="*res://net.tscn"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/mode=3
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[gui]

common/snap_controls_to_pixels=false

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0.247059, 0.247059, 0.247059, 1)
anti_aliasing/quality/screen_space_aa=1
